using System;

class GayTest
{
    static void Main()
    {
        Console.Title = "–§–∞–Ω-–¢–µ—Å—Ç: –ö—Ç–æ —Ç—ã –Ω–∞ —Å–∞–º–æ–º –¥–µ–ª–µ?";
        Console.ForegroundColor = ConsoleColor.Magenta;

        Console.WriteLine("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ —É–ª—å—Ç–∏–º–∞—Ç–∏–≤–Ω—ã–π —Ç–µ—Å—Ç –≤–∞—à–µ–π –æ—Ä–∏–µ–Ω—Ç–∞—Ü–∏–∏!");
        Console.WriteLine("–û—Ç–≤–µ—Ç—å—Ç–µ –Ω–∞ –Ω–µ—Å–∫–æ–ª—å–∫–æ –≤–æ–ø—Ä–æ—Å–æ–≤, –∏ –º—ã –æ–ø—Ä–µ–¥–µ–ª–∏–º –ø—Ä–∞–≤–¥—É...");

        string[] questions =
        {
            "1. –õ—é–±–∏—Ç–µ –ª–∏ –≤—ã —Ä–∞–¥—É–≥—É? (–¥–∞/–Ω–µ—Ç)",
            "2. –ù—Ä–∞–≤—è—Ç—Å—è –ª–∏ –≤–∞–º –º—é–∑–∏–∫–ª—ã? (–¥–∞/–Ω–µ—Ç)",
            "3. –≤—ã –ª—é–±–∏—Ç–µ –Ω–µ–≥—Ä–æ–≤? (–¥–∞/–Ω–µ—Ç)",
            "4. –ª—é–±–∏—Ç–µ –ª–∏ –≤—ã —Ç–æ–ª—Å—Ç—ã–π –¥–∏–ª–¥–æ (–¥–∞–∂–µ –µ—Å–ª–∏ –≤—ã –º—É–∂—á–∏–Ω–∞)? (–¥–∞/–Ω–µ—Ç)",
            "5. –í—ã —Å–º–æ—Ç—Ä–µ–ª–∏ —Ö–æ—Ç—è –±—ã –æ–¥–∏–Ω –≤—ã–ø—É—Å–∫ '–†–µ–∞–ª—å–Ω—ã—Ö –ø–∞—Ü–∞–Ω–æ–≤'? (–¥–∞/–Ω–µ—Ç)"
            "5. –µ—Å—Ç—å –ª–∏ —É –≤–∞—Å –æ–¥–Ω–æ–ø–æ–ª—ã–µ –æ—Ç–Ω–æ—à–µ–Ω–∏—è'?(–¥–∞/–Ω–µ—Ç)"
        };

        foreach (string question in questions)
        {
            Console.WriteLine(question);
            Console.Write("–í–∞—à –æ—Ç–≤–µ—Ç: ");
            Console.ReadLine(); // 
        }

        Console.WriteLine("\n–ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ–º –≤–∞—à–∏ –æ—Ç–≤–µ—Ç—ã —Å –ø–æ–º–æ—â—å—é –ò–ò... ü§ñ");
        System.Threading.Thread.Sleep(2000);

        Random rand = new Random();
        string[] results = { "–ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º! –í—ã ‚Äî –≥–µ–π üè≥Ô∏è‚Äçüåà", "–í—ã ‚Äî –≥–æ—Ä–¥–∞—è –ª–µ—Å–±–∏—è–Ω–∫–∞ üè≥Ô∏è‚Äçüåà" };
        string result = results[rand.Next(results.Length)];

        Console.ForegroundColor = ConsoleColor.Green;
        Console.WriteLine("\n–†–µ–∑—É–ª—å—Ç–∞—Ç —Ç–µ—Å—Ç–∞:");
        Console.WriteLine(result);

        Console.ResetColor();
        Console.WriteLine("\nP.S. –ø–æ–∑–¥—Ä–∞–≤–ª—è—é —É –≤–∞—Å –∞—É—Ç–∏–∑–º 3 —Å—Ç–∞–¥–∏–∏");
        Console.WriteLine("–ù–∞–∂–º–∏—Ç–µ –ª—é–±—É—é –∫–ª–∞–≤–∏—à—É –¥–ª—è –≤—ã—Ö–æ–¥–∞...");
        Console.ReadKey();
    }
}
